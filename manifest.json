{
  "manifest_version": 3,
  "name": "Video Overlay v6.1.0",
  "version": "6.1.0",
  "version_name": "6.1.0 - Manual Control",
  "description": "Näyttää tekstin videon päällä kun painat CTRL+SHIFT+F3",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}